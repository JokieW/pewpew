<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>

  <action label="top">
    <action>
      <repeat>
        <times>6</times>
        <action>
          <fire>
            <direction type="aim">5</direction>
            <speed>2</speed>
            <bullet />
          </fire>
          <fire>
            <direction type="aim">0</direction>
            <speed>1.5</speed>
            <bullet />
          </fire>
          <fire>
            <direction type="aim">-5</direction>
            <speed>1</speed>
            <bullet />
          </fire>
          <fire>
            <direction type="aim">0</direction>
            <speed>0.5</speed>
            <bullet />
          </fire>
          <wait>25</wait>
        </action>
      </repeat>
    </action>
  </action>
</bulletml>
