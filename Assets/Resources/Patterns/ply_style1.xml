<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>

  <action label="top">
    <repeat>
      <times>5</times>
      <action>
        <wait>1</wait>
        <fire>
          <direction type="absolute">180</direction>
          <speed>10</speed>
          <bullet label="playerBullet"></bullet>
        </fire>
      </action>
    </repeat>
    <playsound>playerShot00</playsound>
  </action>
</bulletml>
